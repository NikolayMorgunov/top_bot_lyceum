                   data-gallery="gallery" download="{%=file.name%}">
                   <img src="{%=file.thumbnail_url%}">
                </a>
            {% } %}</td>



            <td class="name">
                <a href="{%=file.url%}"
                   title="{%=file.name%}"
                   data-gallery="{%=file.thumbnail_url&&'gallery'%}"
                   download="{%=file.name%}">{%=file.name%}
                </a>
            </td>



            <td class="size"><span>{%=o.formatFileSize(file.size)%}</span></td>


            <td colspan="2"></td>

        {% } %}


