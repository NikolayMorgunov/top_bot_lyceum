        %}
        <td>
            <button class="btn btn-danger delete" data-type="{%=file.delete_type%}"
                    data-url="{%=file.delete_url%}"
                    data-data='{ "csrfmiddlewaretoken" : "iPTiuyP6wvLCKlKJwNkR1WOKUtVJK8N1" }'
                    {% if (file.delete_with_credentials) { %}
